{
  "name": "pixle",
  "version": "1.0.0",
  "hintConfig": {
    "connector": {
      "name": "webhint"
    },
    "extends": [
      "web-recommended"
    ],
    "hints": {
      "meta-viewport": "error"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "preinstall": "npx npm-force-resolutions",
    "pixles": "ts-node local/pixle-generator/pixle-generator.ts",
    "dev:ssr": "ng run pixle-game:serve-ssr",
    "serve:ssr": "node dist/pixle-game/server/main.js",
    "build:ssr": "ng build && ng run pixle-game:server",
    "prerender": "ng run pixle-game:prerender"
  },
  "resolutions": {
    "glob-parent": "6.0.2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.4",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~14.0.4",
    "@angular/compiler": "~14.0.4",
    "@angular/core": "~14.0.4",
    "@angular/forms": "~14.0.4",
    "@angular/platform-browser": "~14.0.4",
    "@angular/platform-browser-dynamic": "~14.0.4",
    "@angular/platform-server": "~14.0.4",
    "@angular/router": "~14.0.4",
    "@angular/service-worker": "~14.0.4",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@nguniversal/express-engine": "^13.1.1",
    "ajv": "^8.8.2",
    "ajv-errors": "^3.0.0",
    "express": "^4.15.2",
    "glob-parent": "6.0.2",
    "got": "^12.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.4",
    "@angular/cli": "~14.0.4",
    "@angular/compiler-cli": "~14.0.4",
    "@hint/configuration-web-recommended": "^8.2.12",
    "@nguniversal/builders": "^14.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^18.0.1",
    "del": "^6.1.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-minify": "^3.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^4.1.0",
    "hint": "^7.0.1",
    "jasmine-core": "~4.2.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "sass": "^1.53.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^10.8.2",
    "typescript": "~4.7.4"
  }
}
